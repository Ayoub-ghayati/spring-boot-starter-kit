type User {
    firstName: String!
    lastName: String!
    age: Int!
    isAdult: Boolean!
}

type Location {
    resources: String
    mappings: String
    locale: String
}

type Query {
    users: [User]
    locations: [Location]
}

type Mutation {
    createUser(firstName: String!, lastName: String!, age: Int!) : User!
}